{
  "name": "@my/ui",
  "version": "0.0.1",
  "sideEffects": [
    "*.css"
  ],
  "private": true,
  "types": "./src",
  "main": "src/index.tsx",
  "module:jsx": "src",
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build --skip-types",
    "watch": "tamagui-build --skip-types --watch",
    "format": "prettier --write '**/*.{ts,tsx}' '!**/node_modules/**' '!**/*.d.ts'"
  },
  "dependencies": {
    "@my/config": "0.0.1",
    "@tamagui/toast": "^1.101.7",
    "tamagui": "^1.101.7"
  },
  "devDependencies": {
    "@tamagui/build": "^1.101.7",
    "eslint": "^9.3.0",
    "prettier": "^3.3.2"
  }
}
